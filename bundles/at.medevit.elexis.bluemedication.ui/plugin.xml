<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="toolbar:ch.elexis.omnivore.views.OmnivoreView">
         <command
               commandId="at.medevit.elexis.emediplan.ui.blueMedicationUpload"
               icon="rsc/bmed_icon_abgleich.png"
               label="BlueMedication Abgleich"
               style="push"
               tooltip="BlueMedication Abgleich">
            <visibleWhen
                  checkEnabled="false">
               <with
                     variable="org.eclipse.ui.selection">
                  <not>
                     <count
                           value="0">
                     </count>
                  </not>
                  <iterate
                        ifEmpty="false"
                        operator="and">
                     <instanceof
                           value="ch.elexis.omnivore.model.IDocumentHandle">
                     </instanceof>
                  </iterate>
                  <test
                        forcePluginActivation="true"
                        property="at.medevit.elexis.bluemedication.ui.isUploadable">
                  </test>
               </with>
            </visibleWhen>
            <parameter
                  name="at.medevit.elexis.bluemedication.ui.resulttyp"
                  value="chmed">
            </parameter>
         </command>
      </menuContribution>
