   <extension
         point="org.eclipse.ui.commands">
      <command
            defaultHandler="at.medevit.elexis.bluemedication.ui.handler.BlueMedicationUploadHandler"
            id="at.medevit.elexis.emediplan.ui.blueMedicationUpload"
            name="Blue Medication Upload">
         <commandParameter
               id="at.medevit.elexis.bluemedication.ui.resulttyp"
               name="resulttyp"
               optional="false"
               values="chmed, pdf">
         </commandParameter>
      </command>
      <command
            defaultHandler="at.medevit.elexis.bluemedication.ui.handler.BlueMedicationDownloadHandler"
            id="at.medevit.elexis.emediplan.ui.blueMedicationDownload"
            name="Blue Medication Download">
